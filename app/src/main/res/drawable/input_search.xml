<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <corners android:radius="10.0mm" />
            <stroke android:width="@dimen/vs_2" android:color="#ffffffff" />
        </shape>
    </item>
    <item android:state_focused="false">
        <shape>
            <corners android:radius="10.0mm" />
            <stroke android:width="@dimen/vs_2" android:color="#99ffffff" />
        </shape>
    </item>
</selector>